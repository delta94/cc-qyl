{
  "name": "bb01",
  "version": "1.0.0",
  "description": "babel introduction ",
  "main": "index.js",
  "scripts": {
    "test": "test",
    "compile": "babel src --watch  --compilers js:babel-core/register  --out-file dist/bundle.js",
    "bundle": "watchify dist/bundle.js  --ignore-missing=true -o ./index.js",
    "build": "concurrently --kill-others \"npm run compile\" \"npm run bundle\"  ",
    "dev": "npm run build && npm run start"
  },
  "keywords": [
    "babel",
    "es6"
  ],
  "author": "hho",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1"
  },
  "dependencies": {
    "watchify": "^3.11.0",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "concurrently": "^3.5.1",
    "nodemon": "^1.17.1"
  }
}
